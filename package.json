  "license": "MIT",
  "repository": "guiyep/my-form-state",
  "core": "dist/core/index.min.js",
  "core-module": "dist/core/index.min.es.js",
  "react": "dist/react/index.min.js",
  "react-module": "dist/react/index.min.es.js",
  "redux": "dist/redux/index.min.js",
  "redux-module": "dist/redux/index.min.es.js",
